package com.digital.productservice.dto;

public class ProductDto {
	
	private String name;
	
	private String licenseNum;
	
	private double price;
	
	private int quantity;
	
	private String description;
	
	private String manufactureDate;
	
	private String returnwithin;

	public ProductDto(String name, String licenseNum, double price, int quantity, String description,
			String manufactureDate, String returnwithin) {
		super();
		this.name = name;
		this.licenseNum = licenseNum;
		this.price = price;
		this.quantity = quantity;
		this.description = description;
		this.manufactureDate = manufactureDate;
		this.returnwithin = returnwithin;
	}

	public ProductDto() {
		super();
		// TODO Auto-generated constructor stub
	}

	
	
	
}
